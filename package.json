{
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "live-server": "^1.2.2",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.1"
  },
  "scripts": {
    "tailwind": "tailwindcss  -i ./css/main.css -o build/tailwind.css --minify  & live-server  ",
    "dev": " live-server  & postcss css/main.css -o ./build/tailwind.css --minify --watch  ",
    "start": "live-server ./ ",
    "build": "postcss css/main.css -o build/tailwind.css --minify"
  },
  "dependencies": {
    "daisyui": "^2.51.5"
  }
}
